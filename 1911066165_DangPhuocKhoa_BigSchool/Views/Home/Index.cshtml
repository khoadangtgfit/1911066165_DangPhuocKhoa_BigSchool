@model IEnumerable<_1911066165_DangPhuocKhoa_BigSchool.Models.Course>
@{
    ViewBag.Title = "Home Page";
}
<link href="~/Content/Site.css" rel="stylesheet" />
<div class="row">
    <ul class="course">
        @foreach (var course in Model)
        {
            <li>
                <div class="date">
                    <div class="month">
                        @course.DateTime.ToString("MM")
                    </div>
                    <div class="day">
                        @course.DateTime.ToString("dd")
                    </div>
                </div>
                <div class="details">
                    <span class="lecturer">
                        @course.Lecturer.Name
                        
                    </span>
                    <span class="category">
                        @course.Category.Name
                    </span>
                </div>

            </li>
        }

    </ul>
</div>
